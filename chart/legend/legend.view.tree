$mol_chart_legend $mol_list
	graphs /$mol_plot_graph
	graphs_front /$mol_plot_graph
	graph_focused? ""
	rows /
		<= Switch $mol_switch
			value? <=> graph_focused?
			options <= switch_options *
			Option* $mol_button_minor
				sub /
					<= Graph_sample_box* $mol_view
						sub /
							<= Graph_sample* null
					<= Graph_title* $mol_view
						sub /
							<= graph_title* \
