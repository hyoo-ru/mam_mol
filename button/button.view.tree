$mol_button $mol_view
	enabled true
	minimal_height 40
	event *
		^
		click?event <=> event_activate?event <=> event_click?event null
		keypress?event <=> event_key_press?event null 
	attr *
		^
		disabled <= disabled false
		role \button
		tabindex <= tab_index 0
		title <= hint \
	sub /
		<= title -
