$mol_check_list $mol_view
	dictionary? *boolean
	Option* $mol_check
		checked? <=> option_checked*? false
		label <= option_label* /
			<= option_title* \
		enabled <= option_enabled* <= enabled true
		hint <= option_hint* \
		minimal_height 24
	options *string
	icons *$mol_icon
	icon_only false
	keys /string
	sub <= items /$mol_check
