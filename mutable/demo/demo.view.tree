		\articles_mutable.hello.title( prev => prev + '!' )
		\articles_mutable.hello.tags( prev => [ ... prev, 'hello' ] )
		\articles_mutable.hello.author.name( ()=> 'John' )
		\articles_mutable.bye( ()=> ({
		\	title: 'Bye, World!',
		\	tags: [],
		\	author: null,
		\}) )
		\
		\const articles_new = articles_mutable()
