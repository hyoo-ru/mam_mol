	code? \
		\const articles_immutable = {
		\	hello: {
		\		title: 'Hello, World',
		\		tags: [ 'javascript', 'immutablity' ],
		\		author: {
		\			name: 'Jin',
		\		},
		\	},
		\}
		\
		\const articles_mutable = $mol_mutable( articles_immutable )
